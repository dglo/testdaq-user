then
 echo reset > /proc/driver/domhub/card3/pair0/domA/comstat
 echo reset > /proc/driver/domhub/card3/pair0/domB/comstat
 echo reset > /proc/driver/domhub/card3/pair1/domA/comstat
 echo reset > /proc/driver/domhub/card3/pair1/domB/comstat
