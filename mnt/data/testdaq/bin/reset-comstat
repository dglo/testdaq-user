#!/bin/bash
# m krasberg
if [ -e /proc/driver/domhub/card0/pair0/domA/comstat ]
then
 echo reset > /proc/driver/domhub/card0/pair0/domA/comstat
 echo reset > /proc/driver/domhub/card0/pair0/domB/comstat
 echo reset > /proc/driver/domhub/card0/pair1/domA/comstat
 echo reset > /proc/driver/domhub/card0/pair1/domB/comstat
 echo reset > /proc/driver/domhub/card0/pair2/domA/comstat
 echo reset > /proc/driver/domhub/card0/pair2/domB/comstat
 echo reset > /proc/driver/domhub/card0/pair3/domA/comstat
