 echo reset > /proc/driver/domhub/card5/pair2/domB/comstat
 echo reset > /proc/driver/domhub/card5/pair3/domA/comstat
 echo reset > /proc/driver/domhub/card5/pair3/domB/comstat
fi
if [ -e /proc/driver/domhub/card6/pair0/domA/comstat ]
then
 echo reset > /proc/driver/domhub/card6/pair0/domA/comstat
 echo reset > /proc/driver/domhub/card6/pair0/domB/comstat
 echo reset > /proc/driver/domhub/card6/pair1/domA/comstat
 echo reset > /proc/driver/domhub/card6/pair1/domB/comstat
